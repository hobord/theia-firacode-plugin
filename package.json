    "*"
  ],
  "devDependencies": {
    "@theia/plugin": "next",
    "@theia/plugin-packager": "latest",
    "clean-webpack-plugin": "^0.1.19",
    "css-loader": "^3.5.2",
    "file-loader": "^6.0.0",
    "rimraf": "2.6.2",
    "style-loader": "^1.1.4",
    "ts-loader": "^4.1.0",
